{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "no-underscore-dangle": 0
  },
  "globals": {
    "__SERVER__": false
  }
}
